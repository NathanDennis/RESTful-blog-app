<%- include ("partials/header") %>

<h1>All Posts</h1>

<% posts.forEach((post) => { %>
    <div>
        <h2><%= post.title %></h2>
        <img src="<%= post.image %>" alt="">
        <span><%= post.createdAt %></span>
        <p><%= post.body %></p>
    </div>
<% }) %>

<%- include ("partials/footer") %>